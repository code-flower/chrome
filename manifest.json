{
  "manifest_version": 2,
  "name": "codeflower-chrome",
  "version": "0.1",
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["js/jquery.js", "js/index.js"]
  }],
  "web_accessible_resources": ["html/frame.html", "js/frame.js"],
  "icons": { 
    "48": "images/icon48.png",
    "128": "images/icon48.png" 
  }
}