{
  "manifest_version": 2,
  "name": "Codeflower",
  "description": "Visualize github repos.",
  "version": "0.1.1",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["js/jquery.js", "js/main.js"]
  }],
  "web_accessible_resources": ["html/frame.html"],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon48.png"
  }
}